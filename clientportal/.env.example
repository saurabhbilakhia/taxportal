# Copy this file to .env and fill in the values

# Stripe API Keys (get from https://dashboard.stripe.com/test/apikeys)
STRIPE_API_KEY=sk_test_your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# JWT Secret (min 32 characters)
JWT_SECRET=your-256-bit-secret-key-for-jwt-signing-must-be-at-least-32-characters

# Database (for Docker)
POSTGRES_DB=clientportal
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres

# Nanonets Configuration (get from https://app.nanonets.com)
NANONETS_API_KEY=your_nanonets_api_key
NANONETS_MODEL_ID=your_model_id

# Application URL (for webhook callbacks)
APP_BASE_URL=http://localhost:8081

# Notification Configuration
NOTIFICATION_ENABLED=true
ACCOUNTANT_EMAIL=accountant@example.com
FROM_EMAIL=noreply@taxportal.com

# SMTP Configuration (optional - for email notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
